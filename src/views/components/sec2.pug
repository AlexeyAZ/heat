section.sec.sec_2
	div.sec__wrap.sec__wrap_2
		div.agallery.swiper-container.agallery_2.swiper-no-swiping
			div.swiper-wrapper
				each item, index in parallax
					div.swiper-slide.agallery__slide
						div.agallery__elements-container
							div(data-parallax='{"y" : 230, "from-scroll": 200}').agallery__elements
								each iItem, iIndex in item
									- if (iItem[0] === 0) {
										div(class="agallery__element-" + (index + 1) + "_" + (iIndex + 1)
											data-rotate-center=iItem[1]
											data-rotate-speed=iItem[2]).agallery__element.agallery__element_rotate
											img(src=img + "sec2_" + (index + 1) + "/" + (iIndex + 1) + ".png").agallery__element-img
									- }else {
										div(class="agallery__element-" + (index + 1) + "_" + (iIndex + 1) 
										data-parallaxify-range-x=iItem[0] 
										data-parallaxify-range-y=iItem[1]).agallery__element
											img(src=img + "sec2_" + (index + 1) + "/" + (iIndex + 1) + ".png").agallery__element-img
									- }
						div.container.agallery__slide-container
							a(href=links[index] target="_blank")
								img(src=img + "sec2-box_" + (index + 1) + ".png" alt="" class="agallery__open-box_" + (index + 1)).agallery__open-box
			div.agallery__arrow.agallery__arrow_left
			div.agallery__arrow.agallery__arrow_right